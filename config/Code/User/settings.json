// Place your settings in this file to overwrite the default settings
{
  "editor.fontFamily": "'Fira Code', 'PragmataPro', 'Operator Mono Lig Book', PowerlineSymbols, monospace",
  "editor.fontSize": 18,
  "editor.fontLigatures": true,
  "editor.glyphMargin": true,
  "editor.rulers": [80, 120],
  "editor.renderWhitespace": "all",
  "python.pythonPath": "/usr/bin/python3",
  "python.linting.pep8Enabled": false,
  "python.linting.flake8Enabled": true,
  "editor.insertSpaces": true,
  "editor.tabSize": 2,
  "files.eol": "\n",
  "files.autoSave": "onFocusChange",
  "terminal.external.linuxExec": "termite",
  "terminal.integrated.shell.linux": "fish",
  "html.format.endWithNewline": true,
  "extensions.autoUpdate": true,
  "workbench.editor.showTabs": false,
  "workbench.colorTheme": "One Dark Pro",
  "workbench.iconTheme": "material-icon-theme",
  "window.menuBarVisibility": "toggle",
  "material-icon-theme.showUpdateMessage": false,
  "window.zoomLevel": 1,
  "editor.minimap.enabled": false,
  "editor.minimap.maxColumn": 120,
  "editor.minimap.renderCharacters": false,
  "C_Cpp.intelliSenseEngine": "Default",
  "C_Cpp.clang_format_path": "/usr/bin/clang-format",
  "C_Cpp.clang_format_style": "Google",
  "extensions.ignoreRecommendations": false,
  "workbench.startupEditor": "newUntitledFile",
  "explorer.confirmDelete": false,
  "emmet.includeLanguages": {
    "coffeescript": "javascriptreact",
    "coffee": "javascriptreact",
    "javascript": "javascriptreact"
  },
  "git.autofetch": true,
  "standard.options": {
    "parser": "babel-eslint"
  },
  "prettier.singleQuote": true,
  "workbench.statusBar.feedback.visible": false,
  "git.confirmSync": false,
  "materialTheme.fixIconsRunning": false,
  "breadcrumbs.enabled": true
}